{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "dependencyDashboard": true,
  "prHourlyLimit": 2,
  "packageRules": [
    {
      "matchManagers": [
        "gomod"
      ],
      "groupName": "Go modules"
    },
    {
      "matchManagers": [
        "github-actions"
      ],
      "groupName": "GitHub Actions"
    }
  ]
}
